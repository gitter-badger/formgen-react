{
  "author": "hilkenan",
  "name": "formgen-react",
  "version": "0.1.2",
  "description": "Formula generator with JSON for react. UI Fabric controls are used for rendering",
  "license": "MIT",
  "homepage": "https://github.com/hilkenan/formgen-react",
  "main": "Form",
  "module": "es",
  "keywords": ["react", "uifabric", "formula", "genertor", "react-component"],
  "repository": {
    "type": "git",
    "url": "git://github.com/hilkenan/formgen-react.git"
  },
  "bugs": {
    "url": "https://github.com/hilkenan/formgen-react/issues"
  },
  "dependencies": {
    "@types/prop-types": "^15.5.2",
    "@types/react-intl": "^2.3.5",
    "@uifabric/styling": "^5.19.0",
    "browser-locale": "^1.0.3",
    "intl": "^1.2.5",
    "json2typescript": "^1.0.5",
    "moment": "^2.20.1",
    "office-ui-fabric-react": "^5.52.0",
    "rc-cascader": "^0.12.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-input-moment": "^1.7.5",
    "react-input-slider": "^4.0.1",
    "react-intl": "^2.4.0",
    "react-maskedinput": "^4.0.1",
    "react-quill": "^1.2.6",
    "react-scripts-ts": "2.13.0",
    "react-times": "^3.0.0",
    "sinon": "^4.4.2",
    "webpack": "^3.11.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start": "react-scripts-ts start && watch-css",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "^22.1.3",
    "@types/node": "^9.4.6",
    "@types/react": "^16.0.38",
    "@types/react-dom": "^16.0.4",
    "react-times": "^3.0.0",
    "typescript": "^2.7.2"
  }
}
